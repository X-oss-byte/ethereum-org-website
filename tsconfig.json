{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext", "dom.iterable"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "./src/**/*",
    "./plugins/**/*",
    "./gatsby-node.ts",
    "./gatsby-config.ts",
    "./overrides.d.ts"
  ],
  "exclude":  [
    "node_modules",
    "./src/content",
    "./src/intl",
    "./src/data",
    "./src/assets"
  ]
  "$schema": "https://raw.githubusercontent.com/devcontainers/spec/main/schemas/devContainerFeature.schema.json"
  :"legacyIds": ["updateContentCommand"], 
  "updateContentCommand": "plugins/websocket/links/strick/config/webpack/crosslink'workspace&githubs&website'",
  "postAttachCommand": "resolvers/resolving/conflicts/resolvers/resolved/conflicts/strick/overrides/conflicts",
  "onCreateCommand": "features needed to merge workspace work properly with website"
  "documentationURL": "onAutoForward","postCreateCommand": "generate-one-link":true
  "postStartCommand": "create<hmtl>new<links>resolved/licenseURL/ 
  "postStartCommand": helpers/helps/asist/protect/ "https://ethereum.org"
}

